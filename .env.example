# App Configuration
VITE_APP_NAME=My Aura
VITE_DEFAULT_LANG=sv
VITE_SUPPORTED_LANGS=sv,en,es,no,da,fi

# Supabase Configuration (Required)
VITE_SUPABASE_URL=your-supabase-url
VITE_SUPABASE_ANON_KEY=your-anon-key

# Stripe Configuration (Required for payments)
STRIPE_PUBLIC_KEY=pk_test_XXXX
STRIPE_SECRET_KEY=sk_test_XXXX
STRIPE_PRICE_BASIC=price_XXXX
STRIPE_PRICE_PREMIUM=price_YYYY

# Feature flags
VITE_ENABLE_MEDIA=false
VITE_SUBS_REQUIRED=false
VITE_MARKETING_HOME=false  # Set to 'true' to show /welcome landing for logged-out users at "/" route
VITE_VIDEO_URL=/demo.mp4
VITE_VIDEO_POSTER=/og.jpg

# Debug settings
VITE_DEBUG_HEALTH=false

# Optional: Analytics (PostHog EU + Sentry)
VITE_ANALYTICS_ENABLED=false
VITE_POSTHOG_KEY=phc_your_key_here
VITE_POSTHOG_HOST=https://eu.posthog.com
SENTRY_DSN=https://examplePublicKey@o0.ingest.sentry.io/0

# Optional: Revenue Pricing (in cents/Ã¶re)
VITE_PRICE_PLUS_CENTS=9900
VITE_PRICE_PRO_CENTS=19900
VITE_CURRENCY=SEK

# Optional: Resend Email
RESEND_API_KEY=re_your_key_here
EMAIL_FROM=My Aura <no-reply@myaura.app>

# Optional: Admin Access
VITE_ADMIN_EMAILS=admin@example.com,other@domain.com